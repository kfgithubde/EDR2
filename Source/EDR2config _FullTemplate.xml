<?xml version="1.0" encoding="utf-8"?>

<EDR2>
  <General>
      <Localization>de</Localization>
      <XMLfile>MyOwnInit.xml</XMLfile>
  </General>

  <Charts>
    <EDRpreview>
        <TitleX>X-Richtung EDR: Beschleunigung und Geschwindigkeitsänderung</TitleX>
        <TitleY>Y-Richtung EDR: Beschleunigung und Geschwindigkeitsänderung</TitleY>
        <TitleZ>Z-Richtung EDR: Beschleunigung und Geschwindigkeitsänderung</TitleZ>
    </EDRpreview>
    <MMEpreview>
        <TitleX>X-Richtung: MME-Datensatz ungefiltert und 150Hz gefiltert</TitleX>
        <TitleY>Y-Richtung: MME-Datensatz ungefiltert und 150Hz gefiltert</TitleY>
        <TitleZ>Z-Richtung: MME-Datensatz ungefiltert und 150Hz gefiltert</TitleZ>
    </MMEpreview>
    <Assessment>
        <TitleX>Bewertung X-Richtung, Fehlerkorridor</TitleX>
        <TitleY>Bewertung Y-Richtung, Fehlerkorridor</TitleY>
        <TitleZ>Bewertung Z-Richtung, Fehlerkorridor</TitleZ>
        <ColorMME>7</ColorMME>
        <ColorBox>2</ColorBox>
        <LineTypeEDR>3</LineTypeEDR>
        <SymbolEDR>6</SymbolEDR>
    </Assessment>
  </Charts>


  <Graph2D>
	<Names>
		<G2dGraphic>EDR2: Plotfenster</G2dGraphic>
		<G2dHardcopy>HDC%03i.jpg</G2dHardcopy>
	</Names>

	<Layout>
		<G2dGraphicPosX>1</G2dGraphicPosX>
		<G2dGraphicPosY>3</G2dGraphicPosY>
		<G2dGraphicSizeX>98</G2dGraphicSizeX>
		<G2dGraphicSizeY>85</G2dGraphicSizeY>
		<G2dLinCol>1</G2dLinCol>
		<G2dTxtCol>1</G2dTxtCol>
		<G2dBckCol>0</G2dBckCol>
	</Layout>

	<Messages>
		<G2dHdcOpen>Graph2D HARDCOPY: Öffnen der Datei fehlgeschlagen.</G2dHdcOpen>
		<G2dHdcOpenL>5</G2dHdcOpenL>

		<G2dHdcWrite>Graph2D HARDCOPY: Schreibfehler.</G2dHdcWrite>
		<G2dHdcWriteL>5</G2dHdcWriteL>

		<G2dUser>%s</G2dUser>
		<G2dUserL>5</G2dUserL>

		<G2dUser2>%s</G2dUser2>
		<G2dUser2L>5</G2dUser2L>

		<G2dHdcActive>Hardcopy %s wird erzeugt.</G2dHdcActive>
		<G2dHdcActiveL>1</G2dHdcActiveL>

		<G2dPressAny>Drücke eine beliebige Taste um fortzufahren.</G2dPressAny>
		<G2dPressAnyL>5</G2dPressAnyL>

		<G2dExit>Drücke eine beliebige Taste um das Fenster zu schliessen.</G2dExit>
		<G2dExitL>5</G2dExitL>

		<G2dJouCreate>GRAPH2D Journal-File nicht erzeugt. Fehlernr.: %i.</G2dJouCreate>
		<G2dJouCreateL>5</G2dJouCreateL>

		<G2dJouEntry>GRAPH2D Journal-Listeneintrag nicht erzeugt.</G2dJouEntry>
		<G2dJouEntryL>5</G2dJouEntryL>

		<G2dJouAdd>GRAPH2D Listeneintrag nicht an Journal angehängt.</G2dJouAdd>
		<G2dJouAddL>5</G2dJouAddL>

		<G2dXMLerror>GRAPH2D Fehler in der XML-Datei: %s.</G2dXMLerror>
		<G2dXMLerrorL>5</G2dXMLerrorL>

		<G2dXMLopen>GRAPH2D Öffnen der XML-Datei %s fehlgeschlagen.</G2dXMLopen>
		<G2dXMLopenL>5</G2dXMLopenL>

		<G2dJouEntryUnknwn>GRAPH2D Unbekannter Action-Code im Journal.</G2dJouEntryUnknwn>
		<G2dJouEntryUnknwnL>1</G2dJouEntryUnknwnL>
	</Messages>

  </Graph2D>
</EDR2>
